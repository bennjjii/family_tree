orm onSubmit={this.handleSubmit}>\n          <label>\n            First name\n            <br />\n            <input\n              type=\"text\"\n              name=\"np_first_name\"\n              autoComplete=\"off\"\n              value={this.state.np_first_name}\n              onChange={this.handleChange}\n            ></input>\n          </label>\n          <br />\n          <label>\n            Middle name\n            <br />\n            <input\n              type=\"text\"\n              autoComplete=\"no\"\n              name=\"np_middle_name\"\n              value={this.state.np_middle_name}\n              onChange={this.handleChange}\n            ></input>\n          </label>\n          <br />\n          <label>\n            Last name\n            <br />\n            <input\n              type=\"text\"\n              autoComplete=\"no\"\n              name=\"np_last_name\"\n              value={this.state.np_last_name}\n              onChange={this.handleChange}\n            ></input>\n          </label>\n          {/* <br />\n          <label htmlFor=\"birthday\">Date of birth</label>\n\n          <br />\n          <DatePicker\n            id=\"birthday\"\n            shouldCloseOnSelect={true}\n            dateFormat=\"dd/MM/yyyy\"\n            showYearDropdown\n            scrollableYearDropdown\n            yearDropdownItemNumber={40}\n            autoComplete=\"off\"\n            onChange={this.handleChangeDate}\n            selected={this.state.d_o_b}\n          />*/}\n          <br />\n\n          <label>\n            Gender\n            <br />\n            <select\n              name=\"np_gender\"\n              value={this.state.np_gender}\n              onChange={this.handleChange}\n            >\n              {\" \"}\n              <option value=\"\" disabled hidden>\n                ---\n              </option>\n              <option>Male</option>\n              <option>Female</option>\n            </select>\n          </label>\n          <br />\n\n          <input type=\"submit\" value=\"Save\"></input>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default NewParent;\n","/srv/family_tree_2/family_tree/client/src/components/Child.js",[],["156","157"],"/srv/family_tree_2/family_tree/client/src/components/Login.js",[],"/srv/family_tree_2/family_tree/client/src/components/Register.js",[],{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","replacedBy":"161"},{"ruleId":"158","replacedBy":"162"},{"ruleId":"160","replacedBy":"163"},{"ruleId":"164","severity":2,"message":"165","line":1,"column":21,"nodeType":"166","messageId":"167","endLine":1,"endColumn":26},{"ruleId":"168","severity":1,"message":"169","line":17,"column":11,"nodeType":"166","messageId":"170","endLine":17,"endColumn":20},{"ruleId":"168","severity":1,"message":"171","line":41,"column":26,"nodeType":"166","messageId":"170","endLine":41,"endColumn":30},{"ruleId":"168","severity":1,"message":"172","line":4,"column":8,"nodeType":"166","messageId":"170","endLine":4,"endColumn":18},{"ruleId":"158","replacedBy":"173"},{"ruleId":"160","replacedBy":"174"},{"ruleId":"168","severity":1,"message":"175","line":3,"column":17,"nodeType":"166","messageId":"170","endLine":3,"endColumn":27},{"ruleId":"168","severity":1,"message":"176","line":3,"column":29,"nodeType":"166","messageId":"170","endLine":3,"endColumn":42},{"ruleId":"168","severity":1,"message":"177","line":3,"column":44,"nodeType":"166","messageId":"170","endLine":3,"endColumn":52},{"ruleId":"168","severity":1,"message":"178","line":8,"column":3,"nodeType":"166","messageId":"170","endLine":8,"endColumn":7},{"ruleId":"168","severity":1,"message":"179","line":10,"column":3,"nodeType":"166","messageId":"170","endLine":10,"endColumn":13},{"ruleId":"168","severity":1,"message":"180","line":11,"column":3,"nodeType":"166","messageId":"170","endLine":11,"endColumn":14},{"ruleId":"168","severity":1,"message":"171","line":41,"column":26,"nodeType":"166","messageId":"170","endLine":41,"endColumn":30}